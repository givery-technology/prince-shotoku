{
  "name": "prince-shotoku",
  "version": "1.0.0",
  "description": "Listen to all public channel message and post them in one place",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn src/main.ts",
    "lint": "eslint --ext .ts src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "github.com:givery-technology/prince-shotoku.git"
  },
  "keywords": [
    "slack",
    "bot",
    "bolt"
  ],
  "author": "takayukioda",
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^2.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^10.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.1.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  }
}
